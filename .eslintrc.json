{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-curly-newline": 0,
    "import/extensions": 0,
    "consistent-return": 0,
    "no-use-before-define": 0,
    "nonblock-statement-body-position": 0,
    "curly": 0,
    "no-underscore-dangle": 0,
    "import/order": 0,
    "no-undef": 0,
    "no-shadow": 0
  }
}
